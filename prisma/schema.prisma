datasource db {\n  provider = "postgresql"\n  url      = env("DATABASE_URL")\n}\n\ngenerator client {\n  provider = "prisma-client-js"\n}\n\nmodel Usuario {\n  id        Int      @id @default(autoincrement())\n  nome      String\n  email     String   @unique\n  senha     String\n  perfil    String\n  createdAt DateTime @default(now())\n}\n\nmodel Motorista {\n  id        Int      @id @default(autoincrement())\n  nome      String\n  cnh       String\n  telefone  String\n  createdAt DateTime @default(now())\n  viagens   Viagem[]\n  comissoes ComissaoMotorista[]\n}\n\nmodel Cliente {\n  id        Int      @id @default(autoincrement())\n  nome      String\n  tipo      String\n  contato   String\n  createdAt DateTime @default(now())\n  viagens   Viagem[]\n}\n\nmodel Veiculo {\n  id        Int      @id @default(autoincrement())\n  placa     String   @unique\n  modelo    String\n  ano       Int\n  createdAt DateTime @default(now())\n  viagens   Viagem[]\n}\n\nmodel Viagem {\n  id           Int      @id @default(autoincrement())\n  origem       String\n  destino      String\n  valor        Float?\n  status       String\n  clienteId    Int\n  motoristaId  Int?\n  veiculoId    Int?\n  createdAt    DateTime @default(now())\n  cliente      Cliente   @relation(fields: [clienteId], references: [id])\n  motorista    Motorista? @relation(fields: [motoristaId], references: [id])\n  veiculo      Veiculo?   @relation(fields: [veiculoId], references: [id])\n  comissoes    ComissaoMotorista[]\n}\n\nmodel ContaPagar {\n  id           Int       @id @default(autoincrement())\n  descricao    String\n  fornecedor   String\n  vencimento   DateTime\n  valor        Float\n  pago         Boolean   @default(false)\n  dataPagamento DateTime?\n  criadoEm     DateTime  @default(now())\n  pagoPorId    Int?\n  pagoPor      Usuario?  @relation(fields: [pagoPorId], references: [id])\n}\n\nmodel ContaReceber {\n  id             Int       @id @default(autoincrement())\n  descricao      String\n  cliente        String\n  vencimento     DateTime\n  valor          Float\n  recebido       Boolean   @default(false)\n  dataRecebimento DateTime?\n  criadoEm       DateTime  @default(now())\n  recebidoPorId  Int?\n  recebidoPor    Usuario?  @relation(fields: [recebidoPorId], references: [id])\n  viagemId       Int?\n  viagem         Viagem?   @relation(fields: [viagemId], references: [id])\n}\n\nmodel ComissaoMotorista {\n  id          Int      @id @default(autoincrement())\n  motoristaId Int\n  viagemId    Int?\n  percentual  Float\n  valorBase   Float?\n  valorPago   Float?\n  dataCriacao DateTime @default(now())\n  pago        Boolean  @default(false)\n\n  motorista   Motorista @relation(fields: [motoristaId], references: [id])\n  viagem      Viagem?   @relation(fields: [viagemId], references: [id])\n}\n